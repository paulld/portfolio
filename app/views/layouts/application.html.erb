    <![endif]-->
  </head>
  <body>
    <header id="header" role="banner">
      <%= render partial: "layouts/header" %>
    </header>

    <main id="main" class="inner cover" role="main">
      <%= yield %>
    </main>

    <footer id="footer" role="footer">
      <%= render partial: "layouts/footer" %>
    </footer>

